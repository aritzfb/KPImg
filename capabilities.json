    "dataViewMappings": [
        {
            "conditions": [
                { "category":{"max":1}, "value": { "max": 1 }   , "target": { "max": 1 } }
                
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "top": { "count":100}
                    }
                },
                "values": {
                    "group":{
                        "by":"series",
                        "select": [
                            {
                                "for": {
                                    "in": "value"
                                }
                                
                            }
                            ,{
                                "for": {
                                    "in": "target"
                                }
                            }
                        ]
